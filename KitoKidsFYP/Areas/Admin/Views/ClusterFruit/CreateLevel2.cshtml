@model KitoKidsFYP.Areas.Admin.ViewModels.ClusterFruitLevel2ViewModel


@{
    ViewData["Title"] = "Create Cluster Of Fruit Level Two";
}

<br />

<div class="text-center">
    <h1 class="display-4">Create Cluster Of Fruit Level Two</h1>
</div>

<br />
<br />



<form method="post" asp-action="CreateLevel2" asp-area="Admin" enctype="multipart/form-data">

    <div id="questionSection" class="container">
        <div class="text-danger" asp-validation-summary="ModelOnly"></div>


        <div class="border container bg-light" style="padding:30px; border-radius:20px;">
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
              <div class="form-group mt-3">
                <label asp-for="ImageUrl" class="control-label"></label>
                <input asp-for="ImageUrl" class="form-control" />
                <span asp-validation-for="ImageUrl" class="text-danger"></span>
            </div>



        </div>
    </div>






    <div class="form-group row">
        <div class="col-4 offset-4">
            <button type="submit" class="btn btn-info form-control">Submit Level One</button>
        </div>
    </div>





</form>

